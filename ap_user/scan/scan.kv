<Check>
    color_active: app.theme_cls.tercearyColor
    ripple_effect: False
    size_hint: None, None
    group: 'group'
    on_active: app.on_checkbox_active(*args)

<QrPrinter>
    id: qr_printer
    name: 'qrprinter'
    MDBoxLayout:
        orientation: 'vertical'
        MDBoxLayout:
            orientation: 'horizontal'
            padding:10
            size_hint_y: .2
            MDIconButton:
                icon: 'keyboard-backspace' 
                pos_hint: {'center_x': .1, 'center_y': .5}
                on_press: 
                    root.parent.current= 'homescreen'
            MDLabel:
                text: 'Imprimir' 
                halign: 'center'
                font_style: "Title"
                bold: True       
                pos_hint: {'center_x': .5, 'center_y': .5}
            MDFabButton:
                style:'standard'
                icon:"printer" 
                color_map: 'surface'
                pos_hint: {"center_x": .8, 'center_y': .5}
                line_color: [0.047058823529411764, 0.403921568627451, 0.5019607843137255, 1.0]
                on_press:
                    root.manager.go_screen('qrprinter',[root.conect_printer()], 'Error de conexi√≥n')

            MDFabButton:
                style:'standard' 
                icon:"package-variant" 
                color_map: 'surface'
                pos_hint: {"center_x": .9 , 'center_y': .5}
                line_color: [0.047058823529411764, 0.403921568627451, 0.5019607843137255, 1.0]
                on_press: 
                    root.list_orders()

        MDDivider:

        MDScrollView:
            do_scroll_x: False
            size_hint_y: .8
            MDList:
                id: mdlist

<ScanAnalyze>

<QrScreen>
    name: 'qrscreen'
    ScanAnalyze:
        id: scan
        aspect_ratio: '16:9'
        extracted_data: self.parent.got_result
    MDBoxLayout:
        orientation: 'horizontal'
        size_hint: (.7, .2)
        pos_hint: {'center_x': .5,'center_y': .1}
        spacing: '15dp'
                            
        MDLabel:
            id: lab_qr
            text: ''
            color: 'white'
            bold: True
            font_style: "Headline"
            role: "small"
            pos_hint: {'center_x': .5,'center_y': .5}
                            

<QrDialog>    
    auto_dismiss: False
    size_hint_x: .9
    MDDialogHeadlineText:
        text: 'Qr'

    MDDialogSupportingText:
        id: lb_box_card

    MDDialogContentContainer:
        orientation:'vertical'

        MDDivider:

        MDListItem:
            id: item_origin_dialog
            MDListItemLeadingIcon:
                icon:"map-marker-radius-outline"
            MDListItemSupportingText:
                id: origin_dialog


        MDListItem:
            id: item_destin_dialog
            MDListItemLeadingIcon:
                icon:"map-marker-radius"
            MDListItemSupportingText:
                id: destin_dialog

        MDDivider:

    MDDialogButtonContainer:
        spacing:"8dp"
        MDButton:
            on_press: 
                root.close_card()
            MDButtonText:
                text:"Cancelar"
                theme_text_color: "Custom"
                text_color: app.theme_cls.tercearyColor

        MDButton:
            id: btn_rec
            disabled: True
            on_press:
                root.manager.progress(root.manager.get_screen(root.manager.current))
            on_release:
                root.manager.go_screen('qrscreen', [root.receive_route()], 'Error') 
            MDButtonIcon:
                icon: 'package-variant-closed-plus'
                theme_icon_color: "Custom"
                icon_color: app.theme_cls.tercearyColor
            MDButtonText:
                text:'Recibir'
                theme_text_color: "Custom"
                text_color: app.theme_cls.tercearyColor